{
  "name": "super-repo-manager",
  "description": "Effortlessly manage Git submodules, subtrees, and metadata with ease using this npm module.",
  "version": "1.0.35",
  "author": "Jason Jafari",
  "bin": {
    "srm": "./bin/run.js",
    "srmd": "./bin/dev.js"
  },
  "bugs": "https://github.com/jafarijason/super-repo-manager/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-autocomplete": "^3.1.5",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3.2.8",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.3.0",
    "child_process": "^1.0.2",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "shelljs": "^0.8.5",
    "simple-git": "^3.25.0",
    "typescript": "^5",
    "util": "^0.12.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/theme.json",
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jafarijason/super-repo-manager",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "srm",
    "dirname": "srm",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-*"
    ],
    "topicSeparator": " ",
    "topics": {},
    "theme": "theme.json",
    "hooks": {
      "prerun": [
        "./dist/hooks/prerun/customPreRun"
      ],
      "postrun": "./dist/hooks/postrun/customPostRun"
    }
  },
  "repository": "https://github.com/jafarijason/super-repo-manager",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "nodemon --watch ./src --exec 'shx rm -rf dist && tsc -b' --verbose --ext ts,js,mjs,cjs,json",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pre-publish": "yarn build && yarn prepack && yarn prepack && git add . && yarn version && git push --follow-tags"
  },
  "types": "dist/index.d.ts"
}
